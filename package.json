{
  "name": "wordbucket-fe",
  "version": "1.0.0",
  "description": "Wordbucket front end",
  "main": "www/index.html",
  "scripts": {
    "build": "vite build",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "test:storybook": "npx test-storybook",
    "test:storybook:ci": "concurrently -k \"npm:storybook\" \"wait-on tcp:6006 && npx test-storybook\"",
    "start": "vite dev",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "npm-run-all lint:prettier lint:eslint lint:stylelint",
    "lint:fix": "npm-run-all lint:prettier:fix lint:eslint:fix lint:stylelint:fix",
    "lint:eslint": "eslint \"**/*.tsx\" \"**/*.ts\"",
    "lint:eslint:fix": "eslint \"**/*.tsx\" \"**/*.ts\" --fix",
    "lint:prettier": "prettier --config .prettierrc.json --check **/*.tsx **/*.ts",
    "lint:prettier:fix": "prettier --config .prettierrc.json --write **/*.tsx **/*.ts",
    "lint:stylelint": "stylelint **/*.scss",
    "lint:stylelint:fix": "stylelint **/*.scss --fix"
  },
  "author": "Greg Skinner",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.5.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.21.3",
    "react-router-dom": "^6.21.3",
    "ts-node": "^10.9.2",
    "vite": "^5.0.12",
    "wordbucket": "github:changlingbob/wordbucket#master"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.10",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/gapi": "^0.0.47",
    "@types/gapi.auth2": "^0.0.60",
    "@types/gapi.client.drive": "^3.0.15",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "storybook": "^7.6.10",
    "stylelint": "16.2",
    "stylelint-config-standard-scss": "^13.0.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0"
  }
}
